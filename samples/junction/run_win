         	-quiet \
        	-doclet com.google.doclava.Doclava \
        	-bootclasspath "$JAVA_HOME/jre/lib/rt.jar;$JAVA_HOME/jre/lib/jsse.jar" \
        	-docletpath ../../build/dist/doclava-0.1.7/doclava-0.1.7.jar \
        	-classpath junction/target/JAVAJunction-0.6.7-SNAPSHOT-jar-with-dependencies.jar \
        	-d build/api \
 	       	-hdf project.name "Junction" \
        	-stubs build/stubs \
        	-apixml build/public_api.xml \
        	@doclava/src-list
